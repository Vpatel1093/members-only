<header class="navbar">
  <div class="container">
    <%= link_to "Members Only", root_path %>
    <% if logged_in? %>
      <%= link_to "Create new post", new_post_path %>
      <%= link_to "Log out", logout_path, method: :delete %>
    <% else %>
      <li><%= link_to "Log In", login_path %></li>
    <% end %>
  </div>
</header>
